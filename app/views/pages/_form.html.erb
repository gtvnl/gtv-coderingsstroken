<%= simple_form_for(@page) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name, label: 'Naam' %>
  </div>

  <div class="form-inputs">
    <%= f.input :num_of_lines, label: 'Aantal regels' %>
  </div>


  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
